@model IEnumerable<LandingPad.Models.Twitter>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LPUser.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TwName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TwTag)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TwOauth)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TwVOauth)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LPUser.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TwName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TwTag)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TwOauth)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TwVOauth)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TwitterID }) |
            @Html.ActionLink("Details", "Details", new { id=item.TwitterID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TwitterID })
        </td>
    </tr>
}

</table>
